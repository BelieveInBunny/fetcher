{
  "name": "fetcher",
  "version": "4.1.1",
  "description": "a media downloader",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start-dev": "yarn build && node dist/index.js",
    "build": "yarn clean && run-p --aggregate-output build:*",
    "build:ts": "tsc",
    "build:frontend": "webpack --color",
    "lint": "run-p --aggregate-output --continue-on-error lint:*",
    "lint:eslint": "eslint --format unix src/",
    "lint:prettier": "prettier --check 'src/**/*.{ts,js}'",
    "lint:stylelint": "stylelint --color 'src/**/*.css'",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "test": "NODE_ENV=test TS_NODE_FILES=true LOG_LEVEL=silent nyc --reporter=text mocha -r ts-node/register --color 'src/**/*.spec.ts' --exit",
    "clean": "rm -rf .nyc_output/ dist/"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "express": "4.17.2",
    "form-data": "4.0.0",
    "got": "11.8.3",
    "irc-colors": "1.5.0",
    "irc-framework": "4.12.1",
    "level": "7.0.1",
    "loglevel": "1.8.0",
    "loglevel-plugin-prefix": "0.8.4",
    "pug": "3.0.2",
    "rss-parser": "3.12.0",
    "sanitize-filename": "1.6.3",
    "source-map-support": "0.5.21",
    "tough-cookie": "4.0.0",
    "tslib": "2.3.1",
    "webtorrent": "1.7.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/express": "4.17.13",
    "@types/levelup": "5.1.0",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.21",
    "@types/sinon": "10.0.8",
    "@types/stream-buffers": "3.0.4",
    "@types/tough-cookie": "4.0.1",
    "@types/webtorrent": "0.109.3",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "chai": "4.3.4",
    "copy-webpack-plugin": "10.2.1",
    "css-loader": "6.5.1",
    "cssnano": "5.0.15",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "jquery": "3.6.0",
    "mini-css-extract-plugin": "2.5.2",
    "mocha": "9.1.4",
    "mock-fs": "5.1.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "postcss": "8.4.5",
    "postcss-loader": "6.2.1",
    "prettier": "2.5.1",
    "proxyquire": "2.1.3",
    "sinon": "12.0.1",
    "stream-buffers": "3.0.2",
    "stylelint": "14.2.0",
    "stylelint-config-standard": "24.0.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "webpack": "5.67.0",
    "webpack-cli": "4.9.1",
    "webpack-manifest-plugin": "4.1.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "all": true
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 150
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": false,
      "jquery": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": "error",
      "max-len": [
        "error",
        {
          "code": 200
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ],
        "extends": [
          "plugin:@typescript-eslint/recommended"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        },
        "rules": {
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/explicit-module-boundary-types": "off",
          "@typescript-eslint/no-empty-function": "off"
        }
      }
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
